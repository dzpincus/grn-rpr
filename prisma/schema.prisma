datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model Post {
  id          Int    @id @default(autoincrement())
  authorEmail String
  title       String
  content     String @db.LongText
}
